class house:
    def __init__(self, name, number_of_floors):
        self.name = name
        self.number_of_floors = number_of_floors
    def go_to (new_floor):
        go_to = int(new_floor)
        if go_to >= 1 and go_to <= new_floor:
            print (go_to)
        else:
            print("Такого этажа не сущществует")
h1 = house('ЖК Горский', 18)
h2 = house('Домик в деревне', 2)
print(h1)
print(h2)
